{
  "version": 2,
  "name": "clockit",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://clockit-gvm2.onrender.com/api/$1"
    },
    {
      "source": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}